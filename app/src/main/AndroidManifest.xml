<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ej.employeeleavesystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".emp_leave_emp_regiter"></activity>
        <activity android:name=".emp_leave_forgot" />
        <activity android:name=".emp_leave_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".emp_leave_home"
            android:label="@string/title_activity_emp_leave_home"
            android:parentActivityName=".emp_leave_home"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".emp_leave_tabs.emp_leave_emp_leave_history"></activity>
    </application>

</manifest>